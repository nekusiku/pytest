pi_test.getStyles()
  styleId = api_test.insertStyle("large_red")
  api_test.getStyle(styleId)
  api_test.updateStyle(styleId)
  api_test.deleteStyle(styleId)

  api_test.getTemplates()
  templateId = api_test.insertTemplate()
